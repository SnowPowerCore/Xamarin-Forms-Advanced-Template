<?xml version="1.0" encoding="utf-8" ?>
<controls:HostedTabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
                           xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                           xmlns:controls="clr-namespace:AppHosting.Xamarin.Forms.Controls;assembly=AppHosting.Xamarin.Forms"
                           xmlns:pages="clr-namespace:XamarinFormsAdvancedTemplate.Views.Pages"
                           x:Class="XamarinFormsAdvancedTemplate.Views.Tabbed.AppTabbedPage">
    <NavigationPage Title="Apples">
        <x:Arguments>
            <pages:AppleListPage Routing.Route="appleListPage"/>
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Other page">
        <x:Arguments>
            <pages:OtherPage Routing.Route="otherPage"/>
        </x:Arguments>
    </NavigationPage>
</controls:HostedTabbedPage>